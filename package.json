{
  "name": "ooco-blog",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf docs/.vuepress/.temp docs/.vuepress/.cache dist",
    "dev": "vuepress dev docs --clean-cache",
    "build": "yarn clean && vuepress build docs --clean-cache --dest dist",
    "serve": "anywhere -h localhost -d dist",
    "deploy": "bash deploy.sh"
  },
  "devDependencies": {
    "anywhere": "^1.5.0",
    "rimraf": "^3.0.2",
    "vuepress": "2.0.0-beta.39",
    "vuepress-theme-gungnir": "2.0.0-alpha.22"
  },
  "dependencies": {
    "@mdit-vue/plugin-toc": "^0.11.0",
    "npm": "^8.18.0"
  }
}
